{
  "manifest_version": 3,
  "name": "Özel Video Hızlandırıcı",
  "version": "1.0",
  "description": "Video oynatma hızını 100x'e kadar zorla ayarlar.",
  "permissions": [
    "storage",      
    "activeTab",    
    "scripting"     
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}